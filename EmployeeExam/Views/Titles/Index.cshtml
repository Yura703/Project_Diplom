@model IEnumerable<EmployeeExam.Data.Entities.Title>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.title_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.var_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.gr_security)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.interval)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.title_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.var_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.gr_security)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.interval)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.title_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.title_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.title_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
